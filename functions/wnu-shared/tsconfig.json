{
    "include": ["src"],
    "compileOnSave": true,
    "compilerOptions":
    {
        "target": "es5",
        "module": "commonjs",
        "declaration": true,
        "sourceMap": true,
        "outDir": "out",
        "esModuleInterop": true,
        "baseUrl": "src",
        "lib": ["es6"],
        "noImplicitReturns": true,
        "moduleResolution": "node"
    }
}